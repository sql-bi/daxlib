	VAR SvgTemplate = MAXX(FilteredTable, [SvgTemplate])
	VAR FinalSvgString =
		SUBSTITUTE(SvgTemplate, "{color}", hex)
	RETURN
		FinalSvgString

	annotation DAXLIB_PackageId = charles.iconpack

	annotation DAXLIB_PackageVersion = 0.0.1